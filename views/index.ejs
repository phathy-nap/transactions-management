<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home PAge</title>
    <link rel="stylesheet" href="style.css" />
  </head>
  <body>
    <h1>Transactions</h1>
    <a href="/form">New</a>
    <table>
      <tr>
        <th>id</th>
        <th>product_name</th>
        <th>date</th>
        <th>qty</th>
        <th>total_price</th>
        <th>edit</th>
      </tr>
      <% allTransactionsData.forEach(e => {%>
        <tr>
          <td><%= e.id %></td>
          <td><%= e.product_name %></td>
          <td><%= e.date %></td>
          <td><%= e.qty %></td>
          <td><%= e.total_price %></td>
          <td><a href="/edit/<%= e.id %>">Edit</a></td>
        </tr>
      <%  }); %>
    </table>
  </body>
</html>